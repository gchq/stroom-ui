    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook-js": "start-storybook -p 9001 -c .storybook --ci",
    "storybook": "npm-run-all -p storybook-js watch-css"
